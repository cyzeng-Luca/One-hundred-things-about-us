{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle:ios": "APP_ENV=product node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js  --platform ios --dev false --bundle-output ./ios/bundle/wormHole.jsbundle --assets-dest ./ios/bundle --sourcemap-output ./ios/bundle/wormHole.jsbundle.map"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.16",
    "@types/react-native": "^0.57.51",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "metro-react-native-babel-preset": "^0.54.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.3",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
      "{mock,src,test}/**/*.{js,tsx,json}": [
        "prettier --trailing-comma es5 --single-quote --write",
        "git add"
      ],
      "{mock,src,test}/**/*.{js,tsx}": [
        "eslint --fix",
        "git add"
      ]
  }
}
